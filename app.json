{
  "expo": {
    "scheme": "shelfie",
    "name": "Shelfie",
    "slug": "shelfie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo_dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo_dark.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jomwicked.shelfie"
    },
    "web": {
      "favicon": "./assets/img/logo_dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#e0dfe7",
          "image": "./assets/img/logo_light.png",
          "dark": {
            "image": "./assets/img/logo_dark.png",
            "backgroundColor": "#252331"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7461b4a0-634a-4e58-8891-0c5cd6eddb49"
      }
    },
    "owner": "jomwicked"
  }
}
